sims <- 10000
car <- sample(c("blue","green"), sims, replace = T, prob = c(.15, .85))
ex1 <- function(x){
if(x == "blue"){
val = sample(c("blue","green"), 1, prob = c(.2, .8))
}else{
val = sample(c("blue","green"), 1, prob = c(.2, .8))
}
return(val)
}
actual <- sapply(car, ex1)
df <- data.frame(car, actual)
head(df)
mean(car == "blue" & actual =="blue")
